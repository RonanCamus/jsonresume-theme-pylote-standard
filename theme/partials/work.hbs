<div class="card">
  <div class="card__header">
    <h5>
      Exp√©riences
    </h5>
  </div>
  <div class="card__body">
    {{#each resume.work }}
    <div class="renames">
      <div class="rename__item">
        <div class="rename__avatar rename__avatar--bordered">
          {{{svg 'work'}}}
        </div>
        <div class="rename__title">
          <h6>
            {{ name }}
          </h6>
          <span class="subtitle-1">
            {{ position }}
          </span>
        </div>
        <small class="rename__date">
          {{moment startDate }}
          -
          {{#if endDate}} {{moment endDate}} {{^}} Aujourd'hui {{/if}}
        </small>
      </div>
    </div>

    <p>
      {{breaklines summary }}
    </p>

    {{#not @last}}
    <hr>
    {{/not}}
    {{/each}}
  </div>
</div>